{
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"requireConfigFile": false,
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"plugins": [
		"react"
	],
	"rules": {
		"indent": ["error", "tab", {"SwitchCase": 1}]
		//"no-console": 0
	},
	"globals": {
		"$": true,
		"console": true
	}
}